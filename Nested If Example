vii.	if("ScopingDateCalc" > 365, 'Upcoming', if("ScopingDateCalc" > 31, 'Upcoming', if("ScopingDateCalc" > 0, 'Upcoming', if("ScopingDateCalc" > -31,'Acitve',if("ScopingDateCalc" < -30, 'Recently Closed',if( "TestField" = 'Analysis Completed','Upcoming', if( "ChangeDateChar" = 'On Hold', 'On Hold', if( "ChangeDateChar" = 'In ProgressDEIS', 'Upcoming',if( "ChangeDateChar" = 'In ProgressNOI ', 'Upcoming', if( "ProjectName" ='', 'No current projects on the SOPA', if("CommentDateCalc">-32, 'Active', if("CommentDateCalc"<-31,'Recently Closed', '')))))))))))) 
